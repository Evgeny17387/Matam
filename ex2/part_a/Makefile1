CC = g++
OBJS = part_a_test.o examDetails.o
CFLAGS = -std=c++11 -Wall -Werror -pedantic-errors -g
EXEC = test

$(EXEC): $(OBJS)
	$(CC) $(CFLAGS) $(OBJS) -o $@

part_a_test.o: part_a_test.cpp sortedList.h examDetails.h
	$(CC) $(CFLAGS) -c $*.cpp

examDetails.o: examDetails.cpp examDetails.h
	$(CC) $(CFLAGS) -c $*.cpp

clean:
	rm -f $(OBJS)
